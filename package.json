{
  "name": "graplaweb",
  "version": "1.0.0",
  "type": "module",
  "description": "A beautiful static site built with AgentStatic",
  "main": "src/index.js",
  "scripts": {
    "dev": "agentstatic dev",
    "build": "agentstatic build",
    "build:pages": "node scripts/build-site.js",
    "preview": "agentstatic preview",
    "deploy": "agentstatic deploy",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src content",
    "format:check": "prettier --check src content",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "static-site",
    "agentstatic",
    "portfolio",
    "blog",
    "typescript",
    "ai-native"
  ],
  "author": "conorluddy",
  "license": "MIT",
  "homepage": "https://conorluddy.github.io/GraplaWeb",
  "repository": {
    "type": "git",
    "url": "https://github.com/conorluddy/GraplaWeb.git"
  },
  "bugs": {
    "url": "https://github.com/conorluddy/GraplaWeb/issues"
  },
  "dependencies": {
    "agentstatic": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5.7.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}